<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kjwon.foodchoice.json.mapper.RegisterKeyword">
    <select id="findLocationByName" resultType="com.kjwon.foodchoice.json.model.LatLongPosition">
        select r.latitude, r.longitude from registerKeyword as r where r.name = #{name};
    </select>
</mapper>
